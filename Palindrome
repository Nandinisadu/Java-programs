import java.io.*;
import java.util.*;
class Palindrome{
public static void main(String[] args) {
		int temp,r,n,sum=0;
		Scanner sc=new Scanner(System.in);
		System.out.println("enter the num:");
		n=sc.nextInt();
		temp=n;
		while(n>0) {
			r=n%10;
			sum=sum*10+r;
			n=n/10;
		}
		if(temp==sum) {
			System.out.println("palindrome");
		}else {
			System.out.println("not a palindrome");
		}

	}
}
